<?xml version="1.0" encoding="UTF-8"?>

<configuration status="INFO">
    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%m%n"/>
        </console>
    </appenders>
    <loggers>
        <root level="DEBUG">
            <appender-ref ref="CONSOLE"/>
        </root>
    </loggers>
</configuration>